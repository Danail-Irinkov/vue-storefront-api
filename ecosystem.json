{
  "apps": [
    {
      "name": "api",
      "script": "./dist/index.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_APP_INSTANCE": "kube",
        "NODE_CONFIG_STRICT_MODE": "true",
        "SEO_USE_URL_DISPATCHER": "false"
      },
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "ignore_watch": ["node_modules"]
    },
    {
      "name": "o2m",
      "script": "./dist/worker/order_to_magento2.js",
      "args": "start",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "ignore_watch": ["node_modules"]
    }
  ]
}
