{
  "apps": [
    {
      "name": "api",
      "script": "npm",
      "args": "run dev",
      "instance_var": "INSTANCE_ID",
      "env": {
        "NODE_ENV": "development",
        "NODE_APP_INSTANCE": "dev",
        "SEO_USE_URL_DISPATCHER":
         "false"
      },
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "ignore_watch": ["node_modules"]
    }
  ]
}
